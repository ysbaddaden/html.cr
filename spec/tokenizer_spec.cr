  end
  it "domjs:Dash in script HTML comment" do
    output = "[[\"Character\",\"<!-- - -->\"]]"
    input = "<!-- - -->"
    assert_lexes input, output, HTML::Lexer::State::SCRIPT_DATA, "__invalid__", false
  end
  it "domjs:Dash less-than in script HTML comment" do
