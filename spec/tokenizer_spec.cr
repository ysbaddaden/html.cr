    assert_lexes input, output, HTML::Lexer::State::RAWTEXT, "xmp", false
    assert_lexes input, output, HTML::Lexer::State::SCRIPT_DATA, "xmp", false
  end
  it "domjs:bad endtag (not matching last start tag)" do
    output = "[[\"Character\",\"</xm>\"]]"
